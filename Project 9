#include<iostream>
#include<conio.h>

using namespace std;

int main()
{
	int a;
	int b;

	cout << "Input the number of columns: ";
	cin >> a;
	cout << "Input the number of rows: ";
	cin >> b;

	cout << endl;
	cout << endl;


	for (int c = 1; c <= b; c++)
	{
		for (int d = 1; d <= a; d++)
		{
			cout << "* ";
		}

		cout << endl;
	}
	
	_getch();
	return 0;
}
